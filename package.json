{
  "name": "yandex-translate-async",
  "version": "1.0.0",
  "description": "Yandex translator SDK",
  "main": "dist/index.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base"
  },
  "scripts": {
    "lint": "eslint src/**",
    "build": "babel --presets es2015 src -d dist",
    "prepublish": "npm run build",
    "start": "node dist/",
    "dev": "./node_modules/.bin/babel-node src/",
    "test-cov": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "test": "babel-node ./node_modules/.bin/_mocha"
  },
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palanik/node-es6-starter.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "N. Palani Kumanan",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "isparta": "^4.0.0",
    "mocha": "^3.5.0",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "ky": "^0.10.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.9",
    "request": "^2.88.0",
    "string-strip-html": "^3.2.0"
  }
}
